  <script>
    var input = document.getElementById('searchInput');
    var resultsDiv = document.getElementById('results');
    var noResultsMessage = document.getElementById('noResults');

    // Initially hide resultsDiv
    resultsDiv.style.display = 'none';

    var allDoctors = Array.from(document.getElementsByClassName('doctor-item'));

    input.onkeyup = function () {
      var filter = input.value.toUpperCase();

      if (filter.trim() === '') {
        // If input is empty, hide the resultsDiv and "No results" message
        resultsDiv.style.display = 'none';
        noResultsMessage.style.display = 'none';
        return;
      }

      var filteredDoctors = allDoctors.filter(function (doctorItem) {
        var name = doctorItem.innerText || doctorItem.textContent;
        return name.toUpperCase().includes(filter);
      });

      // Clear the results div
      resultsDiv.innerHTML = '';

      if (filteredDoctors.length > 0) {
        noResultsMessage.style.display = 'none'; // Hide "No results" message
        resultsDiv.style.display = 'block'; // Show resultsDiv

        // Append only the filtered items
        filteredDoctors.forEach(function (doctorItem) {
          resultsDiv.appendChild(doctorItem.cloneNode(true));
        });
      } else {
        resultsDiv.style.display = 'block'; // Show resultsDiv
        noResultsMessage.style.display = 'block'; // Show "No results" message
      }
    };

  </script>
  
