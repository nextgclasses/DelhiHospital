        </div>
      </div>
    </div>
  </div>
  <!-- End Page Title -->

  <!-- Service Details -->
  <div class="service-details-area ptb-100">
    <div class="container">
      <div class="services-details-img">
        <img style="max-height: 350px;object-fit: contain;" src="<%= service.servicesImage %>"
          alt="Service Details" />
        <h2>
          <%= service.title %>
        </h2>
        <p>
        <p>
          <%- service.description %>
        </p>
      </div>

    </div>
  </div>
  <!-- End Service Details -->


  <!-- Blog -->
  <section class="blog-area pt-100 pb-70">
    <div class="container">
      <div class="section-title">
        <h2>Our Latest Blogs</h2>
      </div>
      <div class="row justify-content-center">
        <% blogs.slice(0,3).forEach(blog => {%>
          <div class="col-sm-6 col-lg-4 wow fadeInUp" data-wow-delay=".3s">
            <div class="blog-item">
              <div class="blog-top">
                <a href="/blog-details">
                  <img src="<%= blog.blogImage %>" alt="Blog" />
                </a>
              </div>
              <div class="blog-bottom">
                <h3>
                  <a href="/blog-details"><%= blog.title  %></a>
                </h3>
                  <p>
                    <%- blog.description %>
                  </p>
                <ul>
                  <li>
                    <a href="/blog-details/<%= blog.id  %>">
                      Read More
                      <i class="icofont-long-arrow-right"></i>
                    </a>
                  </li>
                  <li>
                    <i class="icofont-calendar"></i>
                    <%= new Date(blog.created).toLocaleDateString() %>
                  </li>
                </ul>
              </div>
            </div>
          </div>
        <%});  %>
      </div>

    </div>
  </section>
  <!-- End Blog -->

