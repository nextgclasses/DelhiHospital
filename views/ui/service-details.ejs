    </div>

    <!-- Menu For Desktop Device -->
    <div class="main-nav">
      <div class="container">
        <nav class="navbar navbar-expand-md navbar-light">
          <a class="navbar-brand" href="/">
            <img class="rounded-circle" src="<%= siteSettings[0].logo %>" alt="Logo" />
          </a>
          <div class="collapse navbar-collapse mean-menu" id="navbarSupportedContent">
            <ul class="navbar-nav">
              <li class="nav-item">
                <a href="/" class="nav-link active">Home</a>
              </li>
              <li class="nav-item">
                <a href="/about" class="nav-link">About</a>
              </li>
              <li class="nav-item">
                <a href="/departments" class="nav-link">Departments</a>
              </li>
              <li class="nav-item">
                <a href="/service" class="nav-link">Services</a>
              </li>
              <li class="nav-item">
                <a href="/doctor" class="nav-link">Doctors</a>
              </li>
              <li class="nav-item">
                <a href="/blog" class="nav-link">Blog</a>
              </li>
              <li class="nav-item">
                <a href="/contact" class="nav-link">Contact Us</a>
              </li>
            </ul>
            <div class="nav-srh">
              <div class="search-toggle">
                <div class="bg-primary d-none d-lg-flex text-white px-2 py-2 rounded-2">
                  <a class="cmn-btn-right text-white text-nowrap" href="/appointment">Get Appointment</a>
                </div>
                <button class="search-icon icon-close">
                  <i class="icofont-close"></i>
