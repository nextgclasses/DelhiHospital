<body>
  <!-- Header Top -->
  <div class="header-top position-relative" style="z-index: 9999;">
    <div class="fixed-icons position-fixed z-3">
      <a target="_blank" href="tel:<%= siteSettings[0].number %>" class="phone-side-icon mb-1">
        <i class="fas fa-phone-alt fa-2x" style="padding: 2px;"></i>
      </a>
      <a target="_blank" href="https://wa.me/<%= siteSettings[0].number %>" class="whatsUpp-side-icon mb-0">
        <i class="fab fa-whatsapp fa-2x"></i>
      </a>
    </div>
    <div class="container">
      <div class="row align-items-center justify-content-between">
        <div class="col-sm-9 col-lg-6 col-xl-4">
          <div class="header-top-item">
            <div class="header-top-left">
              <ul>
                <li>
                  <a href="tel:<%= siteSettings[0].number %>">
                    <i class="icofont-ui-call"></i>
                    +91 <%= siteSettings[0].number %>

                  </a>
                </li>
                <li>
                  <a href="mailto:<%= siteSettings[0].email %>">
                    <i class="icofont-ui-message"></i>
                    <%= siteSettings[0].email %>

                  </a>
                </li>
              </ul>
            </div>
          </div>
        </div>
        <div
          class="col-sm-9 col-lg-4 col-xl-3 find_doctor_parent d-flex justify-content-center mb-3 mb-sm-0 position-relative">
          <!-- Search input with icon -->
          <div class="input-group">
            <input type="text" class="form-control" placeholder="Find Your Doctor" id="searchInput">
            <span class="input-group-text"><i class="fas fa-search"></i></span>
          </div>

          <div id="results" class="position-absolute custom-results px-2 hide_scroll">
            <% allDoctors.forEach(function(doctor) { %>
              <a href="/appointment" class="doctor-item name d-block hide_scroll py-1 mb-1 overflow-y-scroll"
                value="<%= doctor.name %>">
                <%= doctor.name %>
              </a>
              <% }); %>
          </div>
          <span id="noResults" class="position-absolute custom-no-results">No doctor found</span>

        </div>



        <div class="col-sm-3 col-lg-2 col-xl-3">
          <div class="header-top-item">
            <div class="header-top-right">
              <ul>
                <li>
                  <a href=" <%= siteSettings[0].facebook %>" target="_blank">
                    <i class="icofont-facebook"></i>
                  </a>
                </li>
                <li>
                  <a href=" <%= siteSettings[0].twitter %>" target="_blank">
                    <i class="icofont-twitter"></i>
                  </a>
                </li>
                <li>
                  <a href=" <%= siteSettings[0].pinterest %>" target="_blank">
                    <i class="icofont-pinterest"></i>
                  </a>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- End Header Top -->

  <!-- Start Navbar Area -->
