<%- include('../layout/ui_header.ejs') %>
<!-- Page Title -->
<div class="page-title-area page-title-four">
  <div class="d-table">
    <div class="d-table-cell">
      <div class="page-title-item">
        <h2>Service</h2>
        <ul>
          <li>
            <a href="/">Home</a>
          </li>
          <li>
            <i class="icofont-simple-right"></i>
          </li>
          <li>Service</li>
        </ul>
      </div>
    </div>
  </div>
</div>
<!-- End Page Title -->

<!-- Services -->
<section class="services-area pt-5 pb-70">
  <div class="container">
    <div class="row justify-content-center">
      <% services.forEach((service) => { %>
      <div class="col-sm-6 col-lg-3 wow fadeInUp" data-wow-delay=".3s">
        <div class="service-item">
          <div class="service-front">
            <!-- <i class="icofont-doctor"></i> -->
            <img
              height="50px"
              width="50"
              src="/uploads/<%= service.servicesImage %>"
              alt="Services"
            />
            <h3><%= service.title %></h3>
            <p><%- service.description %></p>
          </div>
          <div class="service-end">
            <img
              height="50px"
              width="50"
              src="/uploads/<%= service.servicesImage %>"
              alt="Services"
            />
            <h3><%= service.title %></h3>
            <p><%- service.description %></p>
            <a href="/service-details/<%= service.id  %>">Read More</a>
          </div>
        </div>
      </div>
      <% }) %>
    </div>
  </div>
</section>
<!-- End Services -->

